{
  "builds": [
    {
      "src": "contact-manager/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "contact-manager/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "DATABASE_URL": "@database_url",
    "SECRET_KEY": "@secret_key"
  }
}

